<Window x:Class="TemplateRevit2025.View.CreatePipe.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:v="clr-namespace:TemplateRevit2025.View.CreatePipe"
        mc:Ignorable="d"
        Title="Main" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <ContentControl Name="ContentTop">
            <v:Top></v:Top>
        </ContentControl>
        <ContentControl Name="ContentMid" Grid.Row="1">
            <v:Mid></v:Mid>
        </ContentControl>
        <Button Content="Ok" Grid.Row="2"></Button>
    </Grid>
</Window>
