<Window x:Class="TemplateRevit2025.View.DuctToAirTerminal.DuctToAirTermialWpf"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TemplateRevit2025.View.DuctToAirTerminal"
             mc:Ignorable="d" 
             Width="400" Height="200">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="35"/>
        </Grid.RowDefinitions>
        <ComboBox Name="ComboboxSizeFlexDuct">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding DuctSize}"/>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
        <Button Click="btnCreateFlexDuct" Margin="0 5 0 0 " HorizontalAlignment="Right" Width="100" Grid.Row="1" Content="Ok"/>
    </Grid>
</Window>
